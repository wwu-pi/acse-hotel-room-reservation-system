- CREATE DATABASE hotel;
- show databases;
- use hotel;
- show tables;
- CREATE TABLE Client(
    ClientNo INT NOT NULL PRIMARY KEY,
    FirstName VARCHAR(20) NOT NULL,
    LastName VARCHAR (20) NOT NULL,
    PhoneNo VARCHAR(20));
- DESCRIBE Client;
- drop table Client;
- INSERT INTO Client
  VALUES (1,'Willy','Winzig', NULL);
